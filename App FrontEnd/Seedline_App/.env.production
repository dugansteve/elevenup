# Production environment variables
# These are used when running `npm run build`

# Admin API disabled in production (no backend server)
VITE_ADMIN_API=

# URL validator disabled in production
VITE_VALIDATOR_URL=

# Main API URL (empty = relative URLs, which will fail gracefully)
VITE_API_URL=

# Activity tracking disabled in production
VITE_ACTIVITY_TRACKING=false
